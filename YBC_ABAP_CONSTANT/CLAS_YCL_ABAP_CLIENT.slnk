      fieldname  = &apos;~HTTP_REMOTE_ADDR&apos;
      fieldindex = 0
    TABLES
      data_tab   = lt_data
    EXCEPTIONS
      OTHERS     = 0.

  READ TABLE lt_data
        REFERENCE INTO ls_data
        INDEX 1.
  IF sy-subrc EQ 0.
    ip_address = ls_data-&gt;line.
  ENDIF.

ENDMETHOD.</source>
