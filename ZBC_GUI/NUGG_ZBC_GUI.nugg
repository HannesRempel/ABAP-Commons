ENDMETHOD.</source>
  </interfaceMethod>
  <method CLSNAME="ZCL_GUI_CUSTOM_CONTAINER" CMPNAME="CONSTRUCTOR" VERSION="1" LANGU="D" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="2" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZCL_GUI_CUSTOM_CONTAINER" CMPNAME="CONSTRUCTOR" SCONAME="CX_SY_CREATE_OBJECT_ERROR" VERSION="1" LANGU="D" DESCRIPT="System-Exception bei Objekterzeugung" MTDTYPE="2" EDITORDER="1 "/>
   <source>METHOD constructor.

  CALL METHOD super-&gt;constructor
    EXPORTING
      container_name          = gv_container
      repid                   = co_repid
      dynnr                   = gv_dynnr
      no_autodef_progid_dynnr = abap_true
    EXCEPTIONS
      OTHERS                  = 1.
  IF sy-subrc NE 0.
    RAISE EXCEPTION TYPE cx_sy_create_object_error
      EXPORTING
        classname = `ZCL_GUI_CUSTOM_CONTAINER`.
  ENDIF.

  SELECT SINGLE text
        FROM trdirt
        INTO gv_title
        WHERE name  EQ sy-cprog
          AND sprsl EQ sy-langu.

  INSERT ucomm_first_page     INTO TABLE gt_fcode.
  INSERT ucomm_last_page      INTO TABLE gt_fcode.
  INSERT ucomm_next_page      INTO TABLE gt_fcode.
  INSERT ucomm_previous_page  INTO TABLE gt_fcode.
  INSERT ucomm_print          INTO TABLE gt_fcode.
  INSERT ucomm_save           INTO TABLE gt_fcode.
  INSERT ucomm_search         INTO TABLE gt_fcode.
  INSERT ucomm_search_next    INTO TABLE gt_fcode.

ENDMETHOD.</source>
  </method>
