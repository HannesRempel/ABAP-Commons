    <include_source>FUNCTION-POOL zbc_gui_custom_control.       &quot;MESSAGE-ID ..</include_source>
   </include>
  </includeprograms>
  <functionmodules>
   <functionmodule NAME="Z_GUI_CUSTOM_CONTROL" STEXT="Show Custom Control">
    <fm_source>DATA: lo_container TYPE REF TO zcl_gui_custom_container
      , lv_dynnr TYPE sydynnr
      .


  lo_container = zcl_gui_custom_container=&gt;get_current( ).
  IF lo_container IS BOUND.
    lv_dynnr = lo_container-&gt;get_dynnr( ).
    CALL SCREEN lv_dynnr.
  ENDIF.</fm_source>
    <functionModuleDocumentation/>
   </functionmodule>
  </functionmodules>
