   <parameter CLSNAME="ZCL_GUI_CUSTOM_CONTAINER" CMPNAME="RAISE_FUNCTION_CODE" SCONAME="I_UCOMM" VERSION="1" LANGU="D" DESCRIPT="Funktionscode, der PAI ausgelöst hat" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="SY-UCOMM"/>
   <source>METHOD raise_function_code.

  IF sr_current EQ me.
    RAISE EVENT function_code
      EXPORTING
        i_ucomm = i_ucomm.
  ENDIF.

ENDMETHOD.</source>
