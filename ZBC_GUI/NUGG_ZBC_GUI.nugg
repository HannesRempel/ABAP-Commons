  <interfaceMethod CLSNAME="ZCL_GUI_CUSTOM_CONTAINER" CPDNAME="ZIF_GUI_FUNCTION~ACTIVATE">
   <source>METHOD zif_gui_function~activate.

  READ TABLE gt_fcode
        WITH KEY table_line = i_ucomm
        BINARY SEARCH
        TRANSPORTING NO FIELDS.

  IF sy-subrc EQ 0.
    DELETE gt_fcode INDEX sy-tabix.
    IF gv_container EQ &apos;C&apos;.
      SET PF-STATUS gv_pfkey OF PROGRAM gv_cprog EXCLUDING gt_fcode.
    ENDIF.
  ENDIF.

ENDMETHOD.</source>
  </interfaceMethod>
  <interfaceMethod CLSNAME="ZCL_GUI_CUSTOM_CONTAINER" CPDNAME="ZIF_GUI_FUNCTION~DEACTIVATE">
   <source>METHOD zif_gui_function~deactivate .

  READ TABLE gt_fcode
        WITH KEY table_line = i_ucomm
        BINARY SEARCH
        TRANSPORTING NO FIELDS.

  IF sy-subrc NE 0.
    INSERT i_ucomm INTO gt_fcode INDEX sy-tabix.
    IF gv_container EQ &apos;C&apos;.
      SET PF-STATUS gv_pfkey OF PROGRAM gv_cprog EXCLUDING gt_fcode.
    ENDIF.
  ENDIF.

ENDMETHOD.</source>
  </interfaceMethod>
  <interfaceMethod CLSNAME="ZCL_GUI_CUSTOM_CONTAINER" CPDNAME="ZIF_GUI_FUNCTION~GET_EXCLUDED">
   <source>METHOD zif_gui_function~get_excluded.

  result = gt_fcode.

ENDMETHOD.</source>
  </interfaceMethod>
  <interfaceMethod CLSNAME="ZCL_GUI_CUSTOM_CONTAINER" CPDNAME="ZIF_GUI_FUNCTION~IS_ACTIVE">
   <source>METHOD zif_gui_function~is_active.

  READ TABLE gt_fcode
        WITH KEY table_line = i_fcode
        BINARY SEARCH
        TRANSPORTING NO FIELDS.
  IF sy-subrc NE 0.
    result = abap_true.
  ENDIF.

ENDMETHOD.</source>
  </interfaceMethod>
