  <source>*******************************************************************
*   System-defined Include-files.                                 *
*******************************************************************
  INCLUDE lzbc_gui_custom_controltop.        &quot; Global Data
  INCLUDE lzbc_gui_custom_controluxx.        &quot; Function Modules

*******************************************************************
*   User-defined Include-files (if necessary).                    *
*******************************************************************
  INCLUDE lzbc_gui_custom_controlf01.        &quot; Subprograms
  INCLUDE lzbc_gui_custom_controlo01.        &quot; PBO-Modules
  INCLUDE lzbc_gui_custom_controli01.        &quot; PAI-Modules</source>
 </mainprogram>
 <includeprograms>
  <include NAME="LZBC_GUI_CUSTOM_CONTROLF01" VARCL="X" SUBC="I" APPL="S" RMAND="200" RLOAD="D" UCCHECK="X">
   <include_source>*&amp;---------------------------------------------------------------------*
*&amp;      Form  pbo
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
FORM pbo.

  DATA: lo_container TYPE REF TO zif_gui_container
      , lv_pfkey TYPE sypfkey
      , lv_cprog TYPE sycprog
      , lt_fcode TYPE rlb_t_excl
      , lv_title TYPE string
      .


  lo_container = zcl_gui_custom_container=&gt;get_current( ).

  lv_pfkey = lo_container-&gt;get_pfkey( ).
  lv_cprog = lo_container-&gt;get_cprog( ).
  lt_fcode = lo_container-&gt;get_excluded_functions( ).
  SET PF-STATUS lv_pfkey OF PROGRAM lv_cprog EXCLUDING lt_fcode.

  lv_title = lo_container-&gt;get_title( ).
  IF lv_title IS NOT INITIAL.
    SET TITLEBAR &apos;TITLE&apos; WITH lv_title.
  ENDIF.

ENDFORM.                    &quot;pbo
*&amp;---------------------------------------------------------------------*
*&amp;      Form  pai
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
FORM pai.

  DATA: lo_container TYPE REF TO zcl_gui_custom_container
      , lv_dynnr TYPE sydynnr
      .


  lo_container = zcl_gui_custom_container=&gt;get_current( ).
  lo_container-&gt;raise_function_code( sy-ucomm ).
  FREE sy-ucomm.

  lv_dynnr = lo_container-&gt;get_dynnr( ).
  SET SCREEN lv_dynnr.

ENDFORM.                    &quot;pai
*&amp;---------------------------------------------------------------------*
*&amp;      Form  exit
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
FORM exit.

  LEAVE TO SCREEN 0.

ENDFORM.                    &quot;exit</include_source>
  </include>
  <include NAME="LZBC_GUI_CUSTOM_CONTROLI01" VARCL="X" SUBC="I" APPL="S" RMAND="200" RLOAD="D" UCCHECK="X">
   <include_source>*----------------------------------------------------------------------*
*  MODULE pai INPUT
*----------------------------------------------------------------------*
*
*----------------------------------------------------------------------*
MODULE pai INPUT.

  PERFORM pai.

ENDMODULE.                    &quot;pai INPUT
*----------------------------------------------------------------------*
*  MODULE exit INPUT
*----------------------------------------------------------------------*
*
*----------------------------------------------------------------------*
MODULE exit INPUT.

  PERFORM pai.
  PERFORM exit.

ENDMODULE.                    &quot;exit INPUT</include_source>
  </include>
  <include NAME="LZBC_GUI_CUSTOM_CONTROLO01" VARCL="X" SUBC="I" APPL="S" RMAND="200" RLOAD="D" UCCHECK="X">
