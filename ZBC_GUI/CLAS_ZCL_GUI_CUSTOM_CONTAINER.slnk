  gv_pfkey = i_pfkey.
  gv_cprog = i_cprog.

ENDMETHOD.</source>
 </interfaceMethod>
 <interfaceMethod CLSNAME="ZCL_GUI_CUSTOM_CONTAINER" CPDNAME="ZIF_GUI_STATUS~SET_TITLE">
