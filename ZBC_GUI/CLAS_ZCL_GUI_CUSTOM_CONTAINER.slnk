 <method CLSNAME="CL_GUI_CUSTOM_CONTAINER" CMPNAME="LINK" VERSION="0" EXPOSURE="0" STATE="0" EDITORDER="0 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
  <source>METHOD link.

  CALL METHOD super-&gt;link
    EXPORTING
      repid                       = co_repid
      dynnr                       = gv_dynnr
      container                   = gv_container
    EXCEPTIONS
      cntl_error                  = 1
      cntl_system_error           = 2
      lifetime_dynpro_dynpro_link = 3.

  CASE sy-subrc.
    WHEN 1.
      RAISE cntl_error.

    WHEN 2.
      RAISE cntl_system_error.

    WHEN 3.
      RAISE lifetime_dynpro_dynpro_link.
  ENDCASE.

ENDMETHOD.</source>
 </method>
 <method CLSNAME="ZCL_GUI_CUSTOM_CONTAINER" CMPNAME="RAISE_FUNCTION_CODE" VERSION="1" LANGU="D" DESCRIPT="Intern only" EXPOSURE="2" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="ZCL_GUI_CUSTOM_CONTAINER" CMPNAME="RAISE_FUNCTION_CODE" SCONAME="I_UCOMM" VERSION="1" LANGU="D" DESCRIPT="Funktionscode, der PAI ausgelÃ¶st hat" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="SY-UCOMM"/>
  <source>METHOD raise_function_code.

  IF sr_current EQ me.
    RAISE EVENT function_code
      EXPORTING
        i_ucomm = i_ucomm.
  ENDIF.

ENDMETHOD.</source>
 </method>
