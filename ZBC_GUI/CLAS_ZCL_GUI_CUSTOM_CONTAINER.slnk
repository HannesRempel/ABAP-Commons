 <interfaceMethod CLSNAME="ZCL_GUI_CUSTOM_CONTAINER" CPDNAME="ZIF_GUI_STATUS~IS_TOOLBAR">
  <source>METHOD zif_gui_status~is_toolbar.

  IF gv_dynnr EQ &apos;9998&apos;.
    result = abap_true.
  ENDIF.

ENDMETHOD.</source>
 </interfaceMethod>
 <interfaceMethod CLSNAME="ZCL_GUI_CUSTOM_CONTAINER" CPDNAME="ZIF_GUI_STATUS~SET_STATUS">
