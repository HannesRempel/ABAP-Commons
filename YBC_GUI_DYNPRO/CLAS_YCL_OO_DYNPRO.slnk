 <method CLSNAME="YCL_OO_DYNPRO" CMPNAME="CONSTRUCTOR" VERSION="1" LANGU="D" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="2" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="YCL_OO_DYNPRO" CMPNAME="CONSTRUCTOR" SCONAME="I_TITLE" VERSION="1" LANGU="D" CMPTYPE="1" MTDTYPE="2" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="CSEQUENCE" PAROPTIONL="X"/>
  <exception CLSNAME="YCL_OO_DYNPRO" CMPNAME="CONSTRUCTOR" SCONAME="CX_SY_CREATE_OBJECT_ERROR" VERSION="1" LANGU="D" DESCRIPT="System-Exception bei Objekterzeugung" MTDTYPE="2" EDITORDER="1 "/>
  <source>METHOD constructor.

  CALL METHOD super-&gt;constructor
    EXPORTING
      container_name          = gv_container
      repid                   = co_repid
      dynnr                   = gv_dynnr
      no_autodef_progid_dynnr = abap_true
    EXCEPTIONS
      OTHERS                  = 1.
  IF sy-subrc NE 0.
    RAISE EXCEPTION TYPE cx_sy_create_object_error
      EXPORTING
        classname = &apos;YCL_OO_DYNPRO&apos;.
  ENDIF.

  IF i_title IS NOT INITIAL.
    gv_title = i_title.

  ELSE.
    SELECT SINGLE text
          FROM trdirt
          INTO gv_title
          WHERE name  EQ sy-cprog
            AND sprsl EQ sy-langu.
  ENDIF.

  INSERT yit_gui_function=&gt;first_page     INTO TABLE gt_fcode.
  INSERT yit_gui_function=&gt;last_page      INTO TABLE gt_fcode.
  INSERT yit_gui_function=&gt;next_page      INTO TABLE gt_fcode.
  INSERT yit_gui_function=&gt;previous_page  INTO TABLE gt_fcode.
  INSERT yit_gui_function=&gt;print          INTO TABLE gt_fcode.
  INSERT yit_gui_function=&gt;save           INTO TABLE gt_fcode.
  INSERT yit_gui_function=&gt;search         INTO TABLE gt_fcode.
  INSERT yit_gui_function=&gt;search_next    INTO TABLE gt_fcode.

ENDMETHOD.</source>
 </method>
