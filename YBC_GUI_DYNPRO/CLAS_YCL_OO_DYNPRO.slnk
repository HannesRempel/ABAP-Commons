ENDMETHOD.</source>
 </interfaceMethod>
 <interfaceMethod CLSNAME="YCL_OO_DYNPRO" CPDNAME="YIF_GUI_FUNCTION~IS_ACTIVE">
  <source>METHOD yif_gui_function~is_active.

  READ TABLE gt_fcode
        WITH KEY table_line = i_fcode
        BINARY SEARCH
        TRANSPORTING NO FIELDS.
  IF sy-subrc NE 0.
    result = abap_true.
  ENDIF.

ENDMETHOD.</source>
 </interfaceMethod>
