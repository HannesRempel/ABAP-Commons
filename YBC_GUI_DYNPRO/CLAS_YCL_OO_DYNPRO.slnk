 <interfaceMethod CLSNAME="YCL_OO_DYNPRO" CPDNAME="YIF_GUI_STATUS~IS_TOOLBAR">
  <source>METHOD yif_gui_status~is_toolbar.

  IF gv_dynnr EQ &apos;9998&apos;.
    result = abap_true.
  ENDIF.

ENDMETHOD.</source>
 </interfaceMethod>
 <interfaceMethod CLSNAME="YCL_OO_DYNPRO" CPDNAME="YIF_GUI_STATUS~SET_STATUS">
  <source>METHOD yif_gui_status~set_status.

  gv_pfkey = i_pfkey.
  gv_cprog = i_cprog.

ENDMETHOD.</source>
 </interfaceMethod>
 <interfaceMethod CLSNAME="YCL_OO_DYNPRO" CPDNAME="YIF_GUI_STATUS~SET_TITLE">
  <source>METHOD yif_gui_status~set_title.

  gv_title = i_title.

ENDMETHOD.</source>
 </interfaceMethod>
 <interfaceMethod CLSNAME="YCL_OO_DYNPRO" CPDNAME="YIF_GUI_STATUS~SET_TOOLBAR">
  <source>METHOD yif_gui_status~set_toolbar.

  IF i_toolbar EQ is_toolbar( ).
    RETURN.
  ENDIF.

  IF i_toolbar EQ abap_true.
    gv_dynnr = &apos;9998&apos;.

  ELSE.
    gv_dynnr = &apos;9999&apos;.
  ENDIF.

  IF gv_container EQ &apos;C&apos;.
    CALL METHOD me-&gt;link
      EXCEPTIONS
        OTHERS = 0.
  ENDIF.

ENDMETHOD.</source>
 </interfaceMethod>
 <interfaceMethod CLSNAME="YCL_OO_DYNPRO" CPDNAME="YIF_OO_DYNPRO~CALL_DYNPRO">
  <source>METHOD yif_oo_dynpro~call_dynpro.

  DATA: lo_current TYPE REF TO ycl_oo_dynpro
      .


  IF parent IS BOUND.
    lo_current = so_current.
    IF lo_current IS BOUND.
      lo_current-&gt;gv_container = &apos;A&apos;.
      CALL METHOD lo_current-&gt;link
        EXCEPTIONS
          OTHERS = 0.
    ENDIF.

    gv_container = &apos;C&apos;.
    CALL METHOD link
      EXCEPTIONS
        OTHERS = 0.

    so_current = me.
    CALL FUNCTION &apos;Y_OO_DYNPRO_CALL&apos;.
    so_current = lo_current.

    gv_container = &apos;A&apos;.
    CALL METHOD link
      EXCEPTIONS
        OTHERS = 0.

    IF lo_current IS BOUND.
      lo_current-&gt;gv_container = &apos;C&apos;.
      CALL METHOD lo_current-&gt;link
        EXCEPTIONS
          OTHERS = 0.
    ENDIF.
  ENDIF.

ENDMETHOD.</source>
 </interfaceMethod>
