  <parameter CLSNAME="YCL_OO_DYNPRO" CMPNAME="RAISE_FUNCTION_CODE" SCONAME="I_UCOMM" VERSION="1" LANGU="D" DESCRIPT="Funktionscode, der PAI ausgelÃ¶st hat" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="SY-UCOMM"/>
  <source>METHOD raise_function_code.

  IF active EQ me.
    RAISE EVENT yif_gui_function~function_code
      EXPORTING
        ucomm = i_ucomm.
  ENDIF.

ENDMETHOD.</source>
