  <includeprograms>
   <include NAME="LYBC_OO_DYNPROF01" VARCL="X" SUBC="I" APPL="S" LEVL="702" RMAND="200" RLOAD="D" UCCHECK="X">
    <include_source>*&amp;---------------------------------------------------------------------*
*&amp;      Form  pbo
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
FORM pbo.

  DATA: lo_dynpro TYPE REF TO yif_oo_dynpro
      , lv_pfkey TYPE sypfkey
      , lv_cprog TYPE sycprog
      , lt_fcode TYPE rlb_t_excl
      , lv_title TYPE string
      .


  lo_dynpro = ycl_oo_dynpro=&gt;get_active( ).

  lv_pfkey = lo_dynpro-&gt;get_pfkey( ).
  lv_cprog = lo_dynpro-&gt;get_cprog( ).
  lt_fcode = lo_dynpro-&gt;get_excluded_functions( ).
  SET PF-STATUS lv_pfkey OF PROGRAM lv_cprog EXCLUDING lt_fcode.

  lv_title = lo_dynpro-&gt;get_title( ).
  IF lv_title IS NOT INITIAL.
    SET TITLEBAR &apos;TITLE&apos; WITH lv_title.
  ENDIF.

ENDFORM.                    &quot;pbo
*&amp;---------------------------------------------------------------------*
*&amp;      Form  pai
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
FORM pai.

  DATA: lo_dynpro TYPE REF TO ycl_oo_dynpro
      , lv_dynnr TYPE sydynnr
      .


  lo_dynpro = ycl_oo_dynpro=&gt;get_active( ).
  lo_dynpro-&gt;raise_function_code( sy-ucomm ).
  CASE sy-ucomm.
    WHEN ycc_gui_function=&gt;back
      OR ycc_gui_function=&gt;cancel
      OR ycc_gui_function=&gt;exit.
      PERFORM exit.

    WHEN OTHERS.
      FREE sy-ucomm.
  ENDCASE.

  lv_dynnr = lo_dynpro-&gt;get_dynnr( ).
  SET SCREEN lv_dynnr.

ENDFORM.                    &quot;pai
*&amp;---------------------------------------------------------------------*
*&amp;      Form  exit
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
FORM exit.

  LEAVE TO SCREEN 0.

ENDFORM.                    &quot;exit</include_source>
   </include>
